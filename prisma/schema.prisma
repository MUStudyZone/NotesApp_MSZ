generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["mongoDb", "dataProxy"]
}

datasource db {
  provider = "mongodb"
  url      = "prisma://aws-us-east-1.prisma-data.com/?api_key=JdfDFhRBWWYcux1hsfSJWvzQTv8ILLcleafcGCYrsaAjeT2cUreg2cd3vvyjvOmn"
